{
  "name": "dr-action",
  "version": "1.0.0",
  "description": "A project to demonstrate GitHub Actions with Google Generative AI API",
  "main": "ai-fix-script.js",
  "scripts": {
    "lint": "eslint .",
    "prettier": "prettier --check ."
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^7.32.0",
    "prettier": "^2.3.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "author": "",
  "license": "ISC"
}
